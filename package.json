  "devDependencies": {
    "express": "4.10.5",
    "fs-extra": "^0.13.0",
    "jshint": "2.8.0",
    "memdown": "^1.0.0",
    "mocha": "^2.0.1",
    "pouchdb": "pouchdb/pouchdb",
    "pouchdb-server": "^0.9.0",
    "supertest": "^0.15.0"
  },
  "scripts": {
    "jshint": "jshint -c .jshintrc lib test",
    "test-pouchdb": "./bin/test-setup.sh && ./bin/test-pouchdb.sh",
